{
  "manifest_version": 2,

  "name": "pipmix",
  "description": "This extension can control pipmix functionality",
  "version": "1.0",

  "browser_action": {
    "default_icon": "pmix_icon.png",
    "default_popup": "pmix.html"
  },
  "permissions": [
    "activeTab",
	"tabs",
	"storage",
	"management"
  ]
}